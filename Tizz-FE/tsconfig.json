{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "downlevelIteration": true,
    "plugins": [
      {
        "name": "next",
      },
      {
        "name": "@0no-co/graphqlsp",
        "schema": "./schema.graphql",
      },
    ],
    "paths": {
      "@/components/*": ["./components/*"],
      "@/utils/*": ["./utils/*"],
      "@/assets/*": ["./assets/*"],
      "@/tizz-components/*": ["./app/[locale]/tizz/_components/*"],
      "@/tizz-trade-components/*": ["./app/[locale]/tizz-trade/_components/*"],
      "@/tizz-trade-hooks/*": ["./app/[locale]/tizz-trade/_hooks/*"],
      "@/tizz-trade-actions/*": ["./app/[locale]/tizz-trade/_actions/*"],
      "@/libraries/*": ["./public/libraries/*"],
      "@/navigation": ["./navigation.ts"],
      "@/types/*": ["./types/*"],
      "@/abis/*": ["./abis/*"],
      "@/proto/*": ["./proto/*"],
      "@/actions/*": ["./app/_actions/*"],
      "@/gql/*": ["./graphql/gql/*"],
    },
  },
  "include": [
    "next-env.d.ts",
    "env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/**/*.ts",
    "**/**/*.tsx",
    ".next/types/**/*.ts",
    "components/inputs/FlatInput",
    "app/tizz/_components/Content6/Content6",
  ],
  "exclude": ["node_modules"],
}
